<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="NumberPopup" t-inherit="point_of_sale.NumberPopup" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('popup-numpad')]/button[text()='1']" position="replace">
                <button class="input-button number-char" t-on-mousedown.prevent="sendInput('100')">100</button>
        </xpath>
    </t>

</templates>
